mixin list(page)
  include meta
  each post in page.posts.toArray()
    li
      .meta
        +meta(post)
      if(theme.excerpt)
        .excerpt
          !=post.excerpt
      if config.disqus_shortname
        include disqus
        +disqus_comments_ticker(post)
  if config.disqus_shortname
    include disqus
    +disqus_comments_script
